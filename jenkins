node {
    stage ('SCM') {
        // git clone
        git 'https://github.com/spring-projects/spring-petclinic.git'
    }
    stage ('build the package') {
        // mvn package
            script: 'mvn clean package'
    }
    stage ('archival') {
        // 'archiveArtifacts'
           archive 'target/*.jar'
    }
}
    
}
